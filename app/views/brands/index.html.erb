<p id="notice"><%= notice %></p>

<h1>Listing Brands</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Vintage</th>
      <th>Asin</th>
      <th>Jan code</th>
      <th>Temperature low</th>
      <th>Temperature high</th>
      <th>Serving temperature</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @brands.each do |brand| %>
      <tr>
        <td><%= brand.name %></td>
        <td><%= brand.vintage %></td>
        <td><%= brand.asin %></td>
        <td><%= brand.jan_code %></td>
        <td><%= brand.temperature_low %></td>
        <td><%= brand.temperature_high %></td>
        <td><%= brand.serving_temperature %></td>
        <td><%= link_to 'Show', brand %></td>
        <td><%= link_to 'Edit', edit_brand_path(brand) %></td>
        <td><%= link_to 'Destroy', brand, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Brand', new_brand_path %>
